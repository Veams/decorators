{
	"name": "@veams/decorators",
	"version": "0.0.1",
	"description": "Decorators for Veams framework.",
	"main": "lib/index.js",
	"module": "lib/index.js",
	"types": "./lib/index.d.ts",
	"scripts": {
		"build": "tsc --project tsconfig.json --outDir lib --declarationDir lib --declaration --module es2015",
		"es-build": "babel lib --out-dir es",
		"cjs-build": "tsc --project tsconfig.json --outDir cjs --declarationDir cjs --declaration --module commonjs",
		"start": "tsc --project tsconfig.json --watch --outDir lib --declarationDir lib --declaration --module es2015",
		"prepublish": "npm run build"
	},
	"devDependencies": {
		"@babel/core": "^7.4.3",
		"@babel/plugin-proposal-class-properties": "^7.0.0",
		"@babel/plugin-proposal-decorators": "^7.0.0",
		"@babel/plugin-proposal-do-expressions": "^7.0.0",
		"@babel/plugin-proposal-export-default-from": "^7.0.0",
		"@babel/plugin-proposal-export-namespace-from": "^7.0.0",
		"@babel/plugin-proposal-function-bind": "^7.0.0",
		"@babel/plugin-proposal-function-sent": "^7.0.0",
		"@babel/plugin-proposal-json-strings": "^7.0.0",
		"@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
		"@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
		"@babel/plugin-proposal-numeric-separator": "^7.0.0",
		"@babel/plugin-proposal-optional-chaining": "^7.0.0",
		"@babel/plugin-proposal-pipeline-operator": "^7.0.0",
		"@babel/plugin-proposal-throw-expressions": "^7.0.0",
		"@babel/plugin-syntax-dynamic-import": "^7.0.0",
		"@babel/plugin-syntax-import-meta": "^7.0.0",
		"@babel/cli": "^7.4.3",
		"@babel/preset-env": "^7.4.3",
		"typescript": "^3.4.3"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/Veams/decorators.git"
	},
	"author": "Sebastian Fitzner",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/Veams/decorators/issues"
	},
	"homepage": "https://github.com/Veams/decorators#readme",
	"dependencies": {
		"rxjs": "^6.4.0"
	}
}
